<Window x:Class="Social_Publisher.PlannerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Social_Publisher"
        mc:Ignorable="d"
        Title="PlannerWindow" Height="600" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="550*"/>
            <RowDefinition Height="50"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0">
            <Frame x:Name="mainFrame" NavigationUIVisibility="Hidden"/>
        </Grid>
        <Grid Grid.Row="1">
            <StackPanel Orientation="Horizontal">
                <Image x:Name="tab1" Source="/Assets/one.png" Height="24" Width="24" Margin="10" MouseUp="tab1_MouseUp"/>
                <Image x:Name="tab2" Source="/Assets/two.png" Height="24" Width="24" Margin="10" MouseUp="tab2_MouseUp"/>
            </StackPanel>
        </Grid>
    </Grid>
</Window>
